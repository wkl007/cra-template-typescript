      "react-router-dom": "^6.0.2",
      "web-vitals": "^2.1.2"
    },
    "devDependencies": {
      "@commitlint/cli": "^15.0.0",
      "@commitlint/config-conventional": "^15.0.0",
      "@craco/craco": "^6.4.2",
      "@testing-library/jest-dom": "^5.16.0",
      "@testing-library/react": "^12.1.2",
      "@testing-library/user-event": "^13.5.0",
      "@types/jest": "^27.0.3",
      "@types/node": "^16.11.11",
      "@types/react": "^17.0.0",
      "@types/react-dom": "^17.0.0",
