      "git-revision-webpack-plugin": "^3.0.6",
      "husky": "^7.0.4",
      "less": "^4.1.2",
      "less-loader": "^10.2.0",
      "lint-staged": "^12.1.2",
      "postcss-html": "^1.3.0",
      "postcss-less": "^5.0.0",
      "prettier": "^2.5.0",
      "react-scripts": "4.0.3",
      "stylelint": "^14.1.0",
      "stylelint-config-html": "^1.0.0",
      "stylelint-config-prettier": "^9.0.3",
      "stylelint-config-rational-order": "^0.1.2",
      "stylelint-config-standard": "^24.0.0",
      "stylelint-order": "^5.0.0",
